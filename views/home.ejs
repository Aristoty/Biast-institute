                        <div class="cource-item">
                            <div class="cource-img">
                                <a href="/courses/<%= filiere[j].dataValues.id %>" title="<%= filiere[j].dataValues.nom_filiere %>"> <img src=<%= "/assets/images/courses/" + filiere[j].dataValues.image_desc %> alt="" /></a>
                                <a class="image-link" href="/courses/<%= filiere[j].dataValues.id %>" title="<%= filiere[j].dataValues.nom_filiere %>">
                                    <i class="fa fa-link"></i>
