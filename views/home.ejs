    <!-- About Us Start --><br/><br/>
    <div id="rs-about" class="rs-about">
        <div class="container">
            <div class="sec-title mb-50 text-center">
                <h2><%= translate('subtitle').content1.title %></h2>     
                <p><%= translate('subtitle').content1.resume %></p>
            </div>
            <div class="row">
                <div class="col-lg-6 col-md-12">
                    <div class="about-img rs-animation-hover">
                        <img src="/assets/images/about/image.png" alt="img02"/>
