<div class="abt-title text-center">
    <h2><%= translate('personalPage').eventDetail %> </h2>
</div>
<!-- Event Details Start -->
<div class="rs-event-details pt-70 pb-70">
    <div class="container">
        <div class="row">
            <!-- <div class="col-lg-8 col-md-12"> -->
                <div class="event-details-content">
                    <h3 class="event-title"><%= translate('database').events[events.dataValues.title]  %></h3>
                    <div class="event-meta">
                        <div class="event-date">
                            <i class="fa fa-calendar"></i>
                            <span><%= events.dataValues.date  %></span>
                        </div>
                        <div class="event-time">
                            <i class="fa fa-clock-o"></i>
                            <span><%= events.dataValues.temps_debut  %> - <%= events.dataValues.temps_fin  %></span>
                        </div>
                        <div class="event-location">
                            <i class="fa fa-map-marker"></i>
                            <span><%= events.dataValues.lieu  %></span>
                        </div>
                    </div>
                    <div style="margin-right: 0%;">
                        <div id="demo" class="carousel slide" data-bs-ride="carousel">

                            <!-- Indicators/dots -->
                            <div class="carousel-indicators">
                              <button type="button" data-bs-target="#demo" data-bs-slide-to="0" class="active"></button>
                              <button type="button" data-bs-target="#demo" data-bs-slide-to="1"></button>
                              <button type="button" data-bs-target="#demo" data-bs-slide-to="2"></button>
                            </div>
                            
                            <!-- The slideshow/carousel -->
                            <div class="carousel-inner" style="border-top-right-radius: 30%; border-bottom-left-radius: 30%; border: 50px inset rgb(205,229,215)">
                                <div class="carousel-item active">
                                    <img src="/assets/images/events/<%= events.dataValues.image_dsc  %>" alt="<%= events.dataValues.image_dsc  %>"class="d-block" style="width:100%">
                                    <div class="carousel-caption">
                                      
                                      <p><%= translate('database').events[events.dataValues.title]  %></p>
                                    </div> 
                                  </div>
                              <div class="carousel-item">
                                <img src="/assets/images/events/<%= events.dataValues.image_dsc  %>" alt="<%= events.dataValues.image_dsc  %>"class="d-block" style="width:100%">
                                <div class="carousel-caption">
                                  
                                  <p><%= translate('database').events[events.dataValues.title]  %></p>
                                </div> 
                              </div>
                              <div class="carousel-item">
                                <img src="/assets/images/events/<%= events.dataValues.image_dsc  %>" alt="<%= events.dataValues.image_dsc  %>" class="d-block" style="width:100%">
                                <div class="carousel-caption">
                                  
                                  <p><%= translate('database').events[events.dataValues.title]  %></p>
                                </div>  
                              </div>
                            </div>
                            
                            <!-- Left and right controls/icons -->
                            <button class="carousel-control-prev" type="button" data-bs-target="#demo" data-bs-slide="prev">
                              <span class="carousel-control-prev-icon"></span>
                            </button>
                            <button class="carousel-control-next" type="button" data-bs-target="#demo" data-bs-slide="next">
                              <span class="carousel-control-next-icon"></span>
                            </button>
                          </div>
                          <p></p>
               
                    <div class="event-desc" style="width: 100%;font-size: large; margin-right: 22%; border-radius: 5px;"  >
                       <%= events.dataValues.description  %>

                    </div>
                </div>
                    
                </div>
            <!-- </div> -->
            
        </div>
    </div>
</div>

<!-- Carousel -->

  


